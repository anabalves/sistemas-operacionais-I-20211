package persistence;

import java.io.BufferedReader;

public class SistemaOperacional {
	private String nameOS;
	private String commandIPOS;
	private String commandPingOS;

	public SistemaOperacional() {
	}
	
	public SistemaOperacional(String nameOS, String commandIPOS, String commandPingOS) {
		this.nameOS = nameOS;
		this.commandIPOS = commandIPOS;
		this.commandPingOS = commandPingOS;
	}

	public String getNameOS() {
		return nameOS;
	}

	public void setNameOS(String nameOS) {
		this.nameOS = nameOS;
	}

	public String getCommandIPOS() {
		return commandIPOS;
	}

	public void setCommandIPOS(String commandIPOS) {
		this.commandIPOS = commandIPOS;
	}

	public String getCommandPingOS() {
		return commandPingOS;
	}

	public void setCommandPingOS(String commandPingOS) {
		this.commandPingOS = commandPingOS;
	}
	
	public String adaptadorEthernetIPv4(BufferedReader buffer) {
		return null;
	}

	public String PingOS(BufferedReader buffer) {
		return null;
	}

}
